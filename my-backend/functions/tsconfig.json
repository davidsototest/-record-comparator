{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "rootDir": "./src",
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "ES6",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
  },
  "compileOnSave": true,
  "include": ["src/**/*.ts", "express-rate-limit.d.ts"],
  "exclude": ["node_modules"]
}